{
	"name": "datacleaning",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "RemovingDuplicates"
				},
				{
					"name": "RemovingBlankAge"
				}
			],
			"scriptLines": [
				"source(output(",
				"          employee_id as integer,",
				"          employee_name as string,",
				"          department as string,",
				"          state as string,",
				"          salary as integer,",
				"          age as integer,",
				"          bonus as integer,",
				"          joining_date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     enableCdc: true,",
				"     mode: 'read',",
				"     skipInitialLoad: false) ~> source1",
				"source1 aggregate(groupBy(employee_id),",
				"     each(match(name!='employee_id'), $$ = first($$))) ~> RemovingDuplicates",
				"RemovingDuplicates filter(!(isNull(age)) && age<120) ~> RemovingBlankAge"
			]
		}
	}
}